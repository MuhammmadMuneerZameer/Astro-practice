import Footer from '../components/Footer.astro';
import Navbar from '../components/Navbar.jsx';
import Layout from '../layouts/Layout.astro';
import '../styles/global.css';
---


<Layout>


  <div class=" min-h-[50vh] flex flex-col items-center justify-center text-center px-4"  style="
  background-image: url(images/bg.png); background-size: cover; background-position: center; background-repeat: no-repeat; background-attachment: fixed;">
    <main  class="max-w-4xl mx-auto py-10 px-4 bg:var(--color-background)">
    <h1 class="font-bold text-3xl mb-4" style="color: var(--color-primary);">
      This is Muneer Blog Section
    </h1>
    <p style="color: var(--color-accent);">
      This was the blog section of the website. Here you can find various articles and posts related to different topics.
    </p>
  </main>
  </div>
</Layout>