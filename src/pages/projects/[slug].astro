---
// import { Astro } from 'astro';

import projects from '../../data/ProjectShowcase.js';

const { slug } = Astro.params;
const project = projects.find(p => p.slug === slug);
---

{project ? (
  <div>
    <h1>{project.title}</h1>
    <p>{project.description}</p>
    <!-- rest of project details -->
  </div>
) : (
  <p>Project not found.</p>
)}
